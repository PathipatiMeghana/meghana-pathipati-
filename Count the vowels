#include<stdio.h>

int main()
{
    char str[100];
    int i,count = 0;
    
    scanf("%s",str);
    
    for(i = 0; str[i] != '\0'; i++)
       'a','e','i','o','u'.
        if( str[i] == 'a' ||
            str[i] == 'e' ||
            str[i] == 'i' || 
            str[i] == 'o' || 
            str[i] == 'u' )
    printf("vowel count = %d\n",count);
    
    return 0;
}
